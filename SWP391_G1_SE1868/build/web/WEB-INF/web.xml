<?xml version="1.0" encoding="UTF-8"?>
<web-app version="6.0" xmlns="https://jakarta.ee/xml/ns/jakartaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_6_0.xsd">
    <servlet>
        <servlet-name>HomePage</servlet-name>
        <servlet-class>controller.HomePage</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>controller.Login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddToWishlistController</servlet-name>
        <servlet-class>controller.AddToWishlistController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ViewWishlistController</servlet-name>
        <servlet-class>wishlist.controller.ViewWishlistController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RemoveFromWishlistController</servlet-name>
        <servlet-class>wishlist.controller.RemoveFromWishlistController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MoveToCartController</servlet-name>
        <servlet-class>wishlist.controller.MoveToCartController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SendOrderStatusNotificationController</servlet-name>
        <servlet-class>notification.controller.SendOrderStatusNotificationController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SubmitSupportRequestController</servlet-name>
        <servlet-class>support.controller.SubmitSupportRequestController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ViewSupportRequestsController</servlet-name>
        <servlet-class>support.controller.ViewSupportRequestsController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UpdateSupportRequestController</servlet-name>
        <servlet-class>support.controller.UpdateSupportRequestController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ChatBotController</servlet-name>
        <servlet-class>support.controller.ChatBotController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ViewChatHistoryController</servlet-name>
        <servlet-class>chatbot.controller.ViewChatHistoryController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>HomePage</servlet-name>
        <url-pattern>/HomePage</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/Login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddToWishlistController</servlet-name>
        <url-pattern>/AddToWishlistController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ViewWishlistController</servlet-name>
        <url-pattern>/ViewWishlistController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RemoveFromWishlistController</servlet-name>
        <url-pattern>/RemoveFromWishlistController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MoveToCartController</servlet-name>
        <url-pattern>/MoveToCartController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SendOrderStatusNotificationController</servlet-name>
        <url-pattern>/SendOrderStatusNotificationController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SubmitSupportRequestController</servlet-name>
        <url-pattern>/SubmitSupportRequestController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ViewSupportRequestsController</servlet-name>
        <url-pattern>/ViewSupportRequestsController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UpdateSupportRequestController</servlet-name>
        <url-pattern>/UpdateSupportRequestController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ChatBotController</servlet-name>
        <url-pattern>/ChatBotController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ViewChatHistoryController</servlet-name>
        <url-pattern>/ViewChatHistoryController</url-pattern>
    </servlet-mapping>
</web-app>
